{
  "name": "aqigetter",
  "version": "1.0.0",
  "description": "Air Quality Index data fetcher from IQAir API with PostgreSQL storage",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "build": "tsc",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "docker:build": "docker build -t aqigetter .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "air-quality",
    "aqi",
    "iqair",
    "cron",
    "postgres",
    "tashkent"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/env": "^5.0.3",
    "@fastify/static": "^8.3.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@fastify/view": "^11.1.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node-cron": "^3.0.11",
    "axios": "^1.13.2",
    "better-sqlite3": "^12.4.6",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "fastify": "^5.6.2",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "pino-pretty": "^13.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "vitest": "^4.0.14"
  }
}
